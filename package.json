{
  "name": "wayfarer",
  "version": "1.0.0",
  "description": "A public bus transportation booking server",
  "main": "index.js",
  "repository": "https://github.com/Deityhub/wayfarer.git",
  "author": "Ojini Chizoba Jude <judoc96@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --recursive test/*",
    "test-dev": "NODE_ENV=test  mocha --require @babel/register --watch --recursive test/*",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon --exec babel-node index.js",
    "dist-build": "babel . --out-dir dist --ignore 'node_modules/'",
    "clean": "rm -rf dist",
    "build": "yarn run clean && yarn run dist-build"
  },
  "engines": {
    "node": "10.16.0",
    "yarn": "1.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcrypt": "^3.0.6",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.11.0",
    "swagger-jsdoc": "^3.3.0",
    "swagger-ui-express": "^4.0.7"
  }
}
